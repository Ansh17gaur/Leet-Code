class Solution(object):
    def isCovered(self, ranges, left, right):
        covered = [False] * 51  # Since 1 <= left, right <= 50

        for start, end in ranges:
            for i in range(start, end + 1):
                covered[i] = True

        for i in range(left, right + 1):
            if not covered[i]:
                return False

        return True
